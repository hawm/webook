<h1 class='d-inline'>Users</h1>
<%= link_to "New user", new_user_path, class: 'btn btn-primary float-end' %>

<ul id="users", class='nav'>
  <% @users.each do |user| %>
    <li id=<%= dom_id user %> class='nav-item' >
      <%= link_to user.username, user, class: 'nav-link' %>
    </li>
  <% end %>
</ul>
